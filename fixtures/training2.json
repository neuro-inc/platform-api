{
  "code": {
    "image": "hagen1778/gif-generator",
    "env": {
      "IMAGE_DIR": "/var/storage/result"
    },
    "storage": [
      {
        "from": "storage://gif-generator/collection",
        "to": "/var/storage/gifs"
      },
      {
        "from": "storage://png-generator/collection",
        "to": "/var/storage/pngs"
      }
    ]
  },
  "resources": {
    "cpus": 2,
    "memoryMb": 128
  }
}